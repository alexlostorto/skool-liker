{
    "manifest_version": 3,
    "name": "Skool Liker",
    "version": "1.0",
    "description": "Automates liking",

    "host_permissions": [
      "*://www.skool.com/*"
    ],
  
    "icons": {
      "128": "logo.png"
    },

    "action": {
      "default_icon": {
        "128": "logo.png"
      },
      "default_title": "Skool Liker",
      "default_popup": "popup.html"
    },
    
    "permissions": [
      "tabs"
    ],

    "content_scripts": [
      {
        "matches": ["*://www.skool.com/*"],
        "js": ["contentScript.js"]
      }
    ]
  
  }
  